{
  "name": "@prefresh/babel-plugin",
  "version": "0.5.1",
  "description": "A babel-plugin to enable fast-refresh for Preact components.",
  "main": "dist/src/index.js",
  "module": "src/index.mjs",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./dist/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "cjyes src/index.mjs",
    "prepublishOnly": "cjyes src/index.mjs",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preactjs/prefresh.git",
    "directory": "packages/babel"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/preactjs/prefresh/issues"
  },
  "homepage": "https://github.com/preactjs/prefresh#readme",
  "devDependencies": {
    "cjyes": "0.3.1"
  },
  "publishConfig": {
    "provenance": true
  }
}
