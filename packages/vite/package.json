{
	"name": "@prefresh/vite",
	"version": "1.0.0",
	"module": "dist/index.mjs",
	"main": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./runtime": {
			"import": "./runtime/index.mjs",
			"require": "./runtime/index.js"
		},
		"./utils": {
			"import": "./utils/index.mjs",
			"require": "./utils/index.js"
		},
		"./package.json": "./package.json",
		"./": "./"
	},
	"modes": {
		"default": "src/index.js",
		"runtime": "src/runtime.js",
		"utils": "src/utils.js"
	},
	"files": [
		"dist",
		"runtime",
		"utils"
	],
	"scripts": {
		"build": "bundt",
		"lint": "eslint src",
		"test": "node test/run-tests.mjs --no-pdf --no-screenshots --tests-glob *.test.mjs",
		"test:debug": "node --inspect test/run-tests.mjs --no-pdf --no-screenshots --tests-glob *.test.mjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JoviDeCroock/prefresh.git",
		"directory": "packages/vite"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/JoviDeCroock/prefresh/issues"
	},
	"homepage": "https://github.com/JoviDeCroock/prefresh#readme",
	"dependencies": {
		"@babel/core": "^7.9.6",
		"@prefresh/core": "^0.8.1",
		"@prefresh/utils": "^0.3.1",
		"react-refresh": "^0.8.2"
	},
	"devDependencies": {
    "@prefresh/vite": "file:./",
		"bundt": "^1.0.1",
		"expect": "^26.3.0",
		"ncp": "^2.0.0",
		"pentf": "^1.1.2",
		"preact": "^10.4.1",
		"puppeteer": "^5.2.1",
		"tmp-promise": "^3.0.2",
		"vite": "^1.0.0-rc.4"
	},
	"peerDependencies": {
		"preact": "^10.4.0",
		"vite": ">=1.0.0-beta.1"
	}
}
