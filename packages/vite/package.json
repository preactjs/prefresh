{
  "name": "@prefresh/vite",
  "version": "2.4.8",
  "description": "A vite plugin to enable fast-refresh for Preact components.",
  "main": "src/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./src/index.js",
      "import": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "modes": {
    "default": "src/index.js"
  },
  "files": [
    "dist",
    "runtime",
    "utils",
    "index.d.ts"
  ],
  "scripts": {
    "lint": "eslint src",
    "test": "jest --clearCache && jest --runInBand --forceExit --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preactjs/prefresh.git",
    "directory": "packages/vite"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/preactjs/prefresh/issues"
  },
  "homepage": "https://github.com/preactjs/prefresh#readme",
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@prefresh/babel-plugin": "0.5.2",
    "@prefresh/core": "^1.5.4",
    "@prefresh/utils": "^1.2.1",
    "@rollup/pluginutils": "^4.2.1"
  },
  "devDependencies": {
    "preact": "^10.15.1",
    "vite": "^5.0.13"
  },
  "peerDependencies": {
    "preact": "^10.4.0 || ^11.0.0",
    "vite": ">=2.0.0"
  },
  "publishConfig": {
    "provenance": true
  }
}
